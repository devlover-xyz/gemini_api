# Server Configuration
PORT=3000
NODE_ENV=development

# Browser Configuration
HEADLESS=true

# Resource Management
BROWSER_POOL_SIZE=5
BROWSER_MAX_IDLE_TIME=300000

# Request Management
MAX_CONCURRENT_REQUESTS=3
REQUESTS_PER_MINUTE=60

# reCAPTCHA Configuration (Optional)
# Leave empty for manual solving, or use one of the services below
RECAPTCHA_API_KEY=
# Options: 2captcha, anti-captcha, manual
RECAPTCHA_PROVIDER=manual
